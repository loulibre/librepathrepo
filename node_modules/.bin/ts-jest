#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jetstart/dev/libre/librepathrepo/node_modules/.pnpm/ts-jest@29.1.0_@babel+core@7.23.0_babel-jest@29.4.1_jest@29.4.1_typescript@5.1.3/node_modules/ts-jest/node_modules:/Users/jetstart/dev/libre/librepathrepo/node_modules/.pnpm/ts-jest@29.1.0_@babel+core@7.23.0_babel-jest@29.4.1_jest@29.4.1_typescript@5.1.3/node_modules:/Users/jetstart/dev/libre/librepathrepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jetstart/dev/libre/librepathrepo/node_modules/.pnpm/ts-jest@29.1.0_@babel+core@7.23.0_babel-jest@29.4.1_jest@29.4.1_typescript@5.1.3/node_modules/ts-jest/node_modules:/Users/jetstart/dev/libre/librepathrepo/node_modules/.pnpm/ts-jest@29.1.0_@babel+core@7.23.0_babel-jest@29.4.1_jest@29.4.1_typescript@5.1.3/node_modules:/Users/jetstart/dev/libre/librepathrepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
